@model System.Collections.Generic.List<ClinicalELDAL.EntityLayer.Appointment>
<div>
    <h3>Approved Appointments</h3>
</div>
<div>
    <div class="col-md-12">
        <table class="table table-condensed table-bordered table-hover">
            <tr>
                <th>Appointment Date</th>
                <th>Doctor Name</th>
                <th>Status</th>
                <th>Message</th>
            </tr>
            @foreach (ClinicalELDAL.EntityLayer.Appointment item in Model)
            {
                <tr>
                    <td>@item.Date</td>
                    <td>@item.User1.Name</td>
                    <td>@item.Status</td>
                    <td>@Html.ActionLink("Message", "Messageview", "Book", new { docid = item.DoctorUserID }, null)</td>
                </tr>
            }
        </table>

    </div>
</div>